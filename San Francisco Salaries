---What were the top 5 jobtitles with the highest paying basepay?


SELECT EmployeeName, JobTitle, BasePay FROM Salaries
WHERE BasePay !="" 
AND EmployeeName !="Not provided"
Order BY BasePay DESC
Limit 5;


--Who were the top 10 employees that earned the highest pay, including overtimepay?
SELECT EmployeeName, JobTitle, TotalPay FROM Salaries
ORDER BY TotalPay DESC
LIMIT 10;


--What is the average base pay and total pay (includes overtime and otherpay) for all the job titles in the San Franciso Fire Department, ?

SELECT JobTitle, ROUND(AVG(BasePay),2) AS BasePay, OvertimePay, OtherPay, ROUND(AVG(TotalPay),2) AS TotalPay FROM Salaries 
WHERE JobTitle LIKE "%fire department%"
GROUP BY JobTitle
ORDER BY BasePay ASC;
